{
  "name": "github-label-copier",
  "version": "0.0.2",
  "description": "simple tool to copy labels from one repository, either your own or someone else's, to your own repository.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "rm -rf lib && tsc -p tsconfig.build.json"
  },
  "keywords": [
    "github",
    "label",
    "copy"
  ],
  "author": "leemhoon00",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leemhoon00/github-label-copier"
  },
  "bugs": {
    "url": "https://github.com/leemhoon00/github-label-copier/issues"
  },
  "files": [
    "LICENSE",
    "README.md",
    "package.json",
    "lib",
    "src"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "yaml": "^2.4.1"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
